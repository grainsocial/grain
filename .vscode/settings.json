{
  "deno.enable": true,
  "deno.lint": true,
  "deno.codeLens.testArgs": ["--allow-all"],
  "editor.defaultFormatter": "denoland.vscode-deno",
  "[typescriptreact]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "[typescript]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "[javascript]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "[html]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "[css]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "[json]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "editor.codeActionsOnSave": {
    "source.organizeImports": "explicit"
  },
  "rust-analyzer.linkedProjects": ["darkroom/Cargo.toml", "aip/Cargo.toml"],
  "dart.lineLength": 100,
  "dart.flutterHotReloadOnSave": "all"
}
