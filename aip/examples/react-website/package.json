{
  "name": "aip-react-demo",
  "version": "1.0.0",
  "description": "React TypeScript demo client for AIP OAuth authentication",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "server": "node server/index.js",
    "server:dev": "tsx watch server/index.ts",
    "build:server": "tsc --project server/tsconfig.json",
    "test:dpop": "tsx server/dpop-test.ts"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "jose": "^5.2.0",
    "node-fetch": "^3.3.2",
    "multiformats": "^13.1.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.2.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/node": "^20.11.0",
    "tsx": "^4.7.0"
  }
}